<UserControl x:Class="AquaSurvivor.UCWin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AquaSurvivor"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label x:Name="labObjectifAtteint" Content="Vous avez atteint votre objectif" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Height="130" Width="800" Background="#FF4FBCBF" FontSize="48" FontWeight="Bold" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <Button x:Name="butRetenter" Content="RETENTER" HorizontalAlignment="Left" Margin="165,326,0,0" VerticalAlignment="Top" Height="83" Width="183" FontWeight="Bold" FontSize="36" Click="butRetenter_Click"/>
        <Button x:Name="but_Quitter" Content="QUITTER" HorizontalAlignment="Left" Margin="452,326,0,0" VerticalAlignment="Top" Height="83" Width="183" FontWeight="Bold" FontSize="36" Click="but_Quitter_Click"/>

    </Grid>
</UserControl>
